<%= render partial: 'shared/errors', locals: {model: @user} %>

<%= form_for @user, url: {action: "create"} do |f| %>
  <%= f.label :nickname %><br />
  <%= f.text_field :nickname %><br />

  <%= f.label :email %><br />
  <%= f.email_field :email %><br />

  <%= f.label :password %><br />
  <%= f.password_field :password %><br />

  <%= f.button :submit %>
<% end %>
